<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.LoginController">
    <children>
        <VBox prefHeight="200.0" prefWidth="500.0">
            <children>
                <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Text fx:id="player1NameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 1 Name  ">
                            <font>
                                <Font name="System Bold" size="24.0" />
                            </font>
                        </Text>
                        <TextField fx:id="player1NameTextField" prefHeight="100.0" prefWidth="270.0" />
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Text fx:id="player2NameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 2 Name  ">
                            <font>
                                <Font name="System Bold" size="24.0" />
                            </font>
                        </Text>
                        <TextField fx:id="player2NameTextField" prefHeight="100.0" prefWidth="270.0" />
                    </children>
                </HBox>
                <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" text="Cancel" />
                        <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#startButtonClicked" text="Start" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
